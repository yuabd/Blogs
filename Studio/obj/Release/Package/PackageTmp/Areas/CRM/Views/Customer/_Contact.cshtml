@model Studio.Models.Customer
		   
<div class="control-group">
	<label class="control-label">联系人 :</label>
	<div class="controls">
		@Html.TextBoxFor(m => m.ContactName, new { @class = "span9", placeholder = "联系人" })
	</div>
</div>
<div class="control-group">
	<label class="control-label">性别 :</label>
	<div class="controls">
		@Html.TextBoxFor(m => m.Gender, new { @class = "span9", placeholder = "性别" })
	</div>
</div>
<div class="control-group">
	<label class="control-label">联系号码 :</label>
	<div class="controls">
		@Html.TextBoxFor(m => m.Phone, new { @class = "span9", placeholder = "联系号码" })
	</div>
</div>
<div class="control-group">
	<label class="control-label">邮箱 :</label>
	<div class="controls">
		@Html.TextBoxFor(m => m.Email, new { @class = "span9", placeholder = "邮箱" })
	</div>
</div>